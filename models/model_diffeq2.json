{
  "constants": {
    "r": 0.05,
    "pi": 0.1,
    "c": 200.0,
    "gamma": 2.5,
    "sigma": 0.5,
    "nu": 2.0
  },
  "arguments": {
    "q": {
      "range": [0.0,1.0],
      "size": 128
    }
  },
  "parameters": {
    "tau": {
      "type": "scalar"
    },
    "g": {
      "type": "scalar"
    }
  },
  "variables": {
    "Wbar": {
      "type": "scalar"
    },  
    "W": {
      "type": "function",
      "arg": "q",
      "deriv": 2
    },
    "x": {
      "type": "function",
      "arg": "q"
    }
  },
  "equations": [
    "(r-g+tau*(1.0-q**nu))*W-pi*q+c*x**gamma*q-(x-g)*q*diff(W,1)-0.5*(x*q*sigma)**2*diff(W,2)",
    "r*Wbar-pi-(gamma-1)*c*(Wbar/(gamma*c))**(gamma/(gamma-1))",
    "gamma*c*x**(gamma-1)-diff(W,1)-x*q*sigma**2*diff(W,2)",
    "W[-1]-Wbar",
    "diff(W,1)[-1]-Wbar"
  ]
}
