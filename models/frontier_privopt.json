{
  "constants": {
    "delta": 0.05,
    "c": 500.0,
    "d": 300.0,
    "gamma": 2.0,
    "nu": 1.0,
    "x": 0.0,
    "y": 0.0
  },
  "arguments": {
    "q": {
      "range": [0.0,1.0],
      "size": 128
    }
  },
  "parameters": {
    "alpha": {
      "type": "scalar"
    },
    "g": {
      "type": "scalar"
    }
  },
  "variables": {
    "W": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "Z": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "Wbar": {
      "type": "scalar"
    },
    "x0": {
      "type": "scalar"
    }
  },
  "equations": [
    "(delta-g+alpha*y*(1.0-q**nu))*W-q+c*((1-alpha)*x0+alpha*x)**gamma*q-(((1-alpha)*x0+alpha*x)-g)*q*diff(W,1)",
    "W[-1]-Wbar",
    "diff(Z,1)+W*nu*q**(nu-1)",
    "Z[-1]",
    "delta*Wbar-1-(gamma-1)*c*(Wbar/(gamma*c))**(gamma/(gamma-1))",
    "gamma*c*x0**(gamma-1)-Wbar"
  ]
}
