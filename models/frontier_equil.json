{
  "constants": [
    "delta",
    "c",
    "d",
    "gamma",
    "beta",
    "Wbar"
  ],
  "arguments": {
    "q": {
      "range": [0.0,1.0],
      "size": 128
    }
  },
  "parameters": {
    "alpha": {
      "type": "scalar"
    },
    "g": {
      "type": "scalar"
    }
  },
  "variables": {
    "W": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "Z": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "x": {
      "type": "function",
      "arg": "q"
    },
    "y": {
      "type": "function",
      "arg": "q"
    },
    "F": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    }
  },
  "equations": [
    "(delta-g+alpha*y*(1-F))*W-q+c*x**gamma*q-(x-g)*q*diff(W,1)",
    "W[-1]-Wbar",
    "diff(Z,1)+W*diff(F,1)",
    "Z[-1]",
    "gamma*c*x**(gamma-1)-diff(W,1)",
    "gamma*d*y**(gamma-1)-Z",
    "diff(F,1)-q*(1-q)**(beta-1)*(beta+2)*(beta+1)/2",
    "F[-1]-1"
  ]
}
