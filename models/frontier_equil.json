{
  "constants": [
    "delta",
    "c",
    "d",
    "gamma",
    "nu",
    "Wbar",
    "x0"
  ],
  "arguments": {
    "q": {
      "range": [0.0,1.0],
      "size": 128
    }
  },
  "parameters": {
    "alpha": {
      "type": "scalar"
    },
    "g": {
      "type": "scalar"
    }
  },
  "variables": {
    "W": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "Z": {
      "type": "function",
      "arg": "q",
      "deriv": 1
    },
    "x": {
      "type": "function",
      "arg": "q"
    },
    "y": {
      "type": "function",
      "arg": "q"
    }
  },
  "equations": [
    "(delta-g+alpha*y*(1.0-q**nu))*W-q+c*x**gamma*q-(x-g)*q*diff(W,1)",
    "W[-1]-Wbar",
    "diff(Z,1)+W*nu*q**(nu-1)",
    "Z[-1]",
    "gamma*c*x**(gamma-1)-diff(W,1)",
    "gamma*d*y**(gamma-1)-Z"
  ]
}
